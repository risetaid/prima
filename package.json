{
  "name": "prima-system",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "dev:next": "next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:profile": "next build --profile",
    "analyze-bundle": "webpack-bundle-analyzer bundle-stats.json",
    "start": "next start",
    "lint": "next lint",
    "precommit": "bun run lint && bunx tsc --noEmit",
    "postinstall": "bun run drizzle-kit generate",
    "db:generate": "bun run drizzle-kit generate",
    "db:migrate": "bun run drizzle-kit migrate",
    "db:push": "bun run drizzle-kit push --force",
    "db:studio": "bun run drizzle-kit studio",
    "nuke-recreate-db": "bun run scripts/nuke-recreate-database.ts",
    "setup-first-user": "bun run scripts/setup-first-user.ts",
    "start-message-worker": "bun run scripts/start-message-worker.ts",
    "db:optimize-indexes": "bun run scripts/optimize-database-indexes.ts",
    "db:monitor-indexes": "bun run scripts/monitor-index-health.ts",
    "test": "bun run test:comprehensive",
    "test:unit": "bunx vitest run",
    "test:unit:watch": "bunx vitest",
    "test:comprehensive": "bun run tests/comprehensive-suite/index.ts",
    "test:auth": "bun run tests/comprehensive-suite/index.ts --category auth",
    "test:reminder": "bun run tests/comprehensive-suite/index.ts --category reminder",
    "test:whatsapp": "bun run tests/comprehensive-suite/index.ts --category whatsapp",
    "test:content": "bun run tests/comprehensive-suite/index.ts --category content",
    "test:load": "bun run tests/comprehensive-suite/index.ts --category load",
    "test:all": "bun run test:auth && bun run test:reminder && bun run test:whatsapp && bun run test:content",
    "test:all:full": "bun run test:all && bun run test:load",
    "test:ci": "bun run test:unit && bun run test:all"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.63.0",
    "@anthropic-ai/tokenizer": "^0.0.4",
    "@clerk/nextjs": "^6.31.6",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-pdf/renderer": "^4.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.33.0",
    "ioredis": "^5.7.0",
    "lucide-react": "^0.539.0",
    "minio": "^8.0.5",
    "next": "^15.4.10",
    "next-pwa": "^5.6.0",
    "postgres": "^3.4.4",
    "quill": "^2.0.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.62.0",
    "sonner": "^2.0.7",
    "svix": "^1.71.0",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/minimatch": "^6.0.0",
    "@types/next-pwa": "^5.6.9",
    "@vitest/ui": "^3.2.4",
    "baseline-browser-mapping": "^2.9.4",
    "dotenv-safe": "^9.1.0",
    "drizzle-kit": "^0.24.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "html-docx-js": "^0.3.1",
    "nock": "^14.0.10",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "trustedDependencies": [
    "@clerk/shared",
    "@tailwindcss/oxide",
    "esbuild",
    "sharp",
    "unrs-resolver"
  ]
}
